{
  "name": "vina-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npx peerjs --port 9000\" \"next dev\"",
    "dev:no-pwa": "concurrently \"npx peerjs --port 9000\" \"NEXT_PUBLIC_DISABLE_PWA=true next dev --turbopack\"",
    "build": "next build",
    "start": "concurrently \"node scripts/start-peerjs-server.js\" \"next start\"",
    "start:peerjs": "node scripts/start-peerjs-server.js",
    "start:next": "next start",
    "start:production": "NODE_ENV=production concurrently \"node scripts/start-peerjs-server.js\" \"next start\"",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "peerjs": "npx peerjs --port 9000",
    "peerjs:prod": "NODE_ENV=production node scripts/start-peerjs-server.js"
  },
  "dependencies": {
    "@types/redis": "^4.0.10",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
