    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "framer-motion": "^12.20.1",
    "iconsax-react": "^0.0.8",
    "lucide-react": "^0.525.0",
    "next": "15.3.2",
    "next-cookies": "^2.0.3",
    "next-intl": "^4.3.5",
    "peerjs": "^1.5.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "recharts": "^3.0.2",
    "redis": "^5.8.2",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.0.10",
    "@storybook/addon-docs": "^9.0.10",
    "@storybook/addon-onboarding": "^9.0.10",
    "@storybook/addon-vitest": "^9.0.10",
    "@storybook/nextjs": "^9.0.10",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/peerjs": "^0.0.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/simple-peer": "^9.11.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-plugin-storybook": "^9.0.10",
    "postcss": "^8.5.6",
    "storybook": "^9.0.10",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
